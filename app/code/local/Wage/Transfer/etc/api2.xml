<config>
    <api2>
        <resource_groups>
            <transfer translate="title" module="Wage_Transfer">
                <title>Content Transfer API</title>
                <sort_order>10</sort_order>
            </transfer>
        </resource_groups>
        <resources>
            <cblocks translate="title" module="Wage_Transfer">
                <group>transfer</group>
                <model>transfer/api2_cblocks</model>
                <title>CMS Blocks</title>
                <sort_order>10</sort_order>
                <privileges>
                    <admin>
                        <create>1</create> 
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
                <attributes module="api2">
                    <items>Items</items>    
                    <block_id>Block Id</block_id>
                    <store_ids>Store Ids</store_ids>
                    <title>Title</title>
                    <identifier>Identifier</identifier>
                    <creation_time>Creation Time</creation_time>
                    <update_time>Update Time</update_time>
                    <content>Content</content>
                    <is_active>Is Active</is_active>
                </attributes>
                <routes>
                    <route_cblocks>
                        <route>/cblocks/</route>
                        <action_type>collection</action_type>
                    </route_cblocks>
                </routes>
                <versions>1</versions>
            </cblocks>
            <cpages translate="title" module="Wage_Transfer">
                <group>transfer</group>
                <model>transfer/api2_cpages</model>
                <title>CMS Pages</title>
                <sort_order>10</sort_order>
                <privileges>
                    <admin>
                        <create>1</create> 
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
                <attributes module="api2">
                    <items>Items</items>  
                    <page_id>page_id</page_id>
                    <store_ids>Store Ids</store_ids>
                    <title>Title</title>
                    <root_template>Root Template</root_template>
                    <meta_keywords>Meta Keywords</meta_keywords>
                    <meta_description>Meta Description</meta_description>
                    <identifier>Identifier</identifier>
                    <content_heading>Content Heading</content_heading>
                    <content>Content</content>
                    <is_active>Is Active</is_active>
                    <sort_order>Sort Order</sort_order>
                    <layout_update_xml>Layout Update Xml</layout_update_xml>
                    <custom_theme>Custom Theme</custom_theme>
                    <custom_root_template>Custom Root Template</custom_root_template>
                    <custom_layout_update_xml>Custom Layout Update Xml</custom_layout_update_xml>
                    <custom_theme_from>Custom Theme From</custom_theme_from>
                    <custom_theme_to>Custom Theme To</custom_theme_to>
                </attributes>
                <routes>
                    <route_cpages>
                        <route>/cpages/</route>
                        <action_type>collection</action_type>
                    </route_cpages>
                </routes>
                <versions>1</versions>
            </cpages>
        </resources>
    </api2>
</config>